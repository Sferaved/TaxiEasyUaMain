<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#434C5E"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:fontFamily="@font/regular"
        android:text="Тип автомобіля"
        android:textColor="#D9D9D9"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/button"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.060000002" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:layout_marginTop="21dp"
        android:fontFamily="@font/regular"
        android:text="Час подачі"
        android:textColor="#D9D9D9"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/editTextText"
        app:layout_constraintTop_toBottomOf="@+id/button2" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="21dp"
        android:fontFamily="@font/regular"
        android:text="Послуги"
        android:textColor="#D9D9D9"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/button5"
        app:layout_constraintTop_toBottomOf="@+id/editTextText" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="21dp"
        android:fontFamily="@font/regular"
        android:text="Коментар"
        android:textColor="#D9D9D9"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/editTextText3"
        app:layout_constraintTop_toBottomOf="@+id/button5" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:fontFamily="@font/regular"
        android:text="Сума"
        android:textColor="#D9D9D9"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/minusbut"
        app:layout_constraintStart_toStartOf="@+id/plusbut"
        app:layout_constraintTop_toBottomOf="@+id/editTextText3" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:layout_marginTop="2dp"
        android:background="@drawable/buttons"
        android:fontFamily="@font/regular"
        android:text="Стандарт"
        android:textColor="#D9D9D9"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@+id/editTextText"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button2"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_marginTop="7dp"
        android:background="@drawable/buttons"
        android:fontFamily="@font/regular"
        android:text="мікроавтобус"
        android:textColor="#D9D9D9"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@+id/editTextText"
        app:layout_constraintTop_toBottomOf="@+id/button" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button5"
        android:layout_width="192dp"
        android:layout_height="40dp"
        android:layout_marginTop="1dp"
        android:background="@drawable/buttons"
        android:fontFamily="@font/regular"
        android:text="До списку послуг"
        android:textColor="#D9D9D9"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@+id/editTextText3"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button8"
        android:layout_width="160dp"
        android:layout_height="70dp"
        android:background="@drawable/buttons"
        android:fontFamily="@font/medium"
        android:text="замовити"
        android:textColor="#D9D9D9"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/minusbut"
        app:layout_constraintEnd_toEndOf="@+id/editTextText3"
        app:layout_constraintTop_toTopOf="@+id/minusbut"
        app:layout_constraintVertical_bias="1.0" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button4"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:layout_marginStart="10dp"
        android:background="@drawable/buttons"
        android:fontFamily="@font/regular"
        android:text="комфорт"
        android:textColor="#D9D9D9"
        android:textSize="15sp"
        app:layout_constraintStart_toEndOf="@+id/button2"
        app:layout_constraintTop_toTopOf="@+id/button2" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button3"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:layout_marginStart="10dp"
        android:background="@drawable/buttons"
        android:fontFamily="@font/regular"
        android:text="універсал"
        android:textColor="#D9D9D9"
        android:textSize="15sp"
        app:layout_constraintStart_toEndOf="@+id/button"
        app:layout_constraintTop_toTopOf="@+id/button" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/inptext"
        android:ems="10"
        android:fontFamily="@font/regular"
        android:hint="__:__"
        android:inputType="text"
        android:textAlignment="center"
        android:textColor="#D9D9D9"
        android:textColorHint="#ABD9D9D9"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/button5"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <EditText
        android:id="@+id/editTextText3"
        android:layout_width="288dp"
        android:layout_height="40dp"
        android:layout_marginTop="1dp"
        android:background="@drawable/inptext"
        android:ems="10"
        android:fontFamily="@font/regular"
        android:hint="З дитячим кріслом"
        android:inputType="text"
        android:textAlignment="viewStart"
        android:textColor="#D9D9D9"
        android:textColorHint="#ABD9D9D9"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/editTextText2"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:background="@drawable/inptext"
        android:ems="10"
        android:fontFamily="@font/regular"
        android:hint="__/__/____"
        android:inputType="text"
        android:textAlignment="center"
        android:textColor="#D9D9D9"
        android:textColorHint="#ABD9D9D9"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/editTextText3"
        app:layout_constraintTop_toTopOf="@+id/editTextText" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:fontFamily="@font/light"
        android:text="Час"
        android:textColor="#9DA0A8"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextText"
        app:layout_constraintStart_toStartOf="@+id/editTextText" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:layout_marginBottom="1dp"
        android:fontFamily="@font/light"
        android:text="Дата"
        android:textColor="#9DA0A8"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextText2"
        app:layout_constraintStart_toStartOf="@+id/editTextText2" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/plusbut"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:layout_marginTop="6dp"
        android:background="@drawable/plus"
        android:fontFamily="@font/regular"
        android:insetLeft="0dp"
        android:insetTop="0dp"
        android:insetRight="0dp"
        android:insetBottom="0dp"
        android:text="+"
        android:textAlignment="center"
        android:textColor="#D9D9D9"
        android:textSize="29sp"
        app:layout_constraintStart_toStartOf="@+id/editTextText3"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

     <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/minusbut"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/minus"
        android:fontFamily="@font/regular"
        android:insetLeft="0dp"
        android:insetTop="0dp"
        android:insetRight="0dp"
        android:insetBottom="0dp"
        android:text="-"
        android:textAlignment="center"
        android:textColor="#D9D9D9"
        android:textSize="35sp"
        app:layout_constraintStart_toEndOf="@+id/plusbut"
        app:layout_constraintTop_toTopOf="@+id/plusbut" />

</androidx.constraintlayout.widget.ConstraintLayout>